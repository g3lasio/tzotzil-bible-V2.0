{
  "expo": {
    "name": "Sistema Nevin",
    "slug": "sistema-nevin",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sistemanevin.app"
    },
    "android": {
      "package": "com.sistemanevin.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "sistema-nevin"
      }
    }
  }
}